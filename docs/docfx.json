{
  "metadata": [
    {
      "src": [
        {
          "src": "../src/",
          "files": [
            "**/bin/**/Altemiq*.dll"
          ],
          "exclude": [
			"**/*.CodeGeneration*/**",
			"**/*.Tests/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appTitle": "Altemiq Core libraries",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/altemiq/runtime"
      },
      "_gitUrlPattern": "github"
	},
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}